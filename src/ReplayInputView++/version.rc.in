
1 VERSIONINFO
    FILEVERSION @IVERSION_COMMA@
    PRODUCTVERSION @VERSION_COMMA@
    FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
    FILEFLAGS 0x1L
#else
    FILEFLAGS 0x0L
#endif
    FILEOS 0x40004L
    FILETYPE 0x02L
    FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "080404b0"
        BEGIN
            VALUE "CompanyName", "SokuDev"
            VALUE "FileDescription", "@PROJECT_DESCRIPTION@"
            VALUE "FileVersion", "@PROJECT_VERSION@"
            VALUE "InternalName", "RIV--"
            VALUE "LegalCopyright", "Copyright (C) 2025 @ Fishuwako"
            VALUE "OriginalFilename", "@PROJECT_NAME@.dll"
            VALUE "ProductName", "@PROJECT_NAME@"
            VALUE "ProductVersion", "@VERSION_DOT@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x804, 1200
    END
END